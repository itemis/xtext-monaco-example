{
  "name": "xtext-monaco-example",
  "description": "Sample Monaco Client to Connect with a Xtext Websocket Language Server",
  "repository": "git@github.com:itemis/xtext-monaco-example.git",
  "version": "0.0.1",
  "vendor": "itemis AG",
  "license": "MIT",
  "dependencies": {
    "express": "^4.15.2",
    "glob-to-regexp": "^0.4.0 ",
    "monaco-languageclient": "^0.6.0",
    "reconnecting-websocket": "^3.2.2",
    "request-light": "^0.2.0",
    "vscode-json-languageservice": "^3.0.12",
    "vscode-languageserver-types": "3.6.1",
    "vscode-languageserver": "^4.2.1",
    "vscode-uri": "^1.0.0",
    "vscode-ws-jsonrpc": "^0.0.1-alpha.3",
    "webpack-cli": "^3.0.7",
    "ws": "^5.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/node": "^10.3.3",
    "@types/ws": "5.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "rimraf": "^2.6.1",
    "typescript": "^2.5.1",
    "webpack": "^4.12.0"
  },
  "scripts": {
    "prepare": "npm run clean && npm run build",
    "compile": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf lib",
    "copy": "cp src/index.html lib/index.html",
    "build": "npm run compile && webpack && npm run copy",
    "start": "npm run build && node lib/server.js",
    "update:file-deps": "npm run clean:file-deps && npm install",
    "clean:file-deps": "npm run clean:monaco-languageclient && npm run clean:vscode-ws-jsonrpc",
    "update:monaco-languageclient": "npm run clean:monaco-languageclient && npm install",
    "clean:monaco-languageclient": "rimraf node_modules/monaco-languageclient",
    "update:vscode-ws-jsonrpc": "npm run clean:vscode-ws-jsonrpc && npm install",
    "clean:vscode-ws-jsonrpc": "rimraf node_modules/vscode-ws-jsonrpc"
  }
}
